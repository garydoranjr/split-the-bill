<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:e="urn:import:com.appspot.splitbill.client.widgets.editor">
	<ui:style>
		.symbol {
			font-weight: bold;
		}
	</ui:style>
	<g:HorizontalPanel ui:field="panel">
		<g:Label ui:field="symbol" styleName='{style.symbol}'>$</g:Label>
		<e:DoubleSumTextBox ui:field="box" />
	</g:HorizontalPanel>
</ui:UiBinder> 