<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="com.appspot.splitbill.client.content.group.table.SpecialBillViewer.MyStyle">
		.panelStyle{
			width: 100%;	
		}
		.panelStyle hr {
			visibility: hidden;
		}
		.label {
			background-color: #92C1F0;
			width: 100%;
  			font-size: 22px;
			font-style: normal;
			font-variant: normal;
  			font-weight: bold;
  			padding-top: 3px;
  			padding-bottom: 3px;
  			padding-left: 5px;
		}
	</ui:style>
	<g:VerticalPanel styleName='{style.panelStyle}' ui:field="panel" />
</ui:UiBinder> 